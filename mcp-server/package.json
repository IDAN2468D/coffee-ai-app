        "express": "^5.2.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.6",
        "@types/node": "^20.0.0",
        "prisma": "^7.3.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.0"
    }
}
